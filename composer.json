{
    "name": "magroski/frogg",
    "description": "Nogg PHP Framework for use with Phalcon Framework",
    "type": "library",
    "keywords": [
        "Phalcon",
        "Framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Magroski",
            "email": "magroski@gmail.com"
        },
        {
            "name": "Tasso Gama",
            "email": "tasso.gama@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Frogg\\": ["src"]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "mobiledetect/mobiledetectlib": "^2.8",
        "magroski/permalink": "^1.0",
        "magroski/wt": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.10.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "4.5",
        "squizlabs/php_codesniffer": "*",
        "phalcon/ide-stubs": "*"
    }
}
